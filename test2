string[] str = new string[]  { "a", "b", "c", "d", "e", "f", "g", "h", "i", "j", 
                               "k", "l", "m", "n", "o", "p", "q", "r", "s", "t", 
                               "u", "v", "w", "x", "y", "z"}; 
string tempStr = "";
int i, j;
Random ran = new Random();
for(i=0;i<10000;i++){
   j = ran.Next(0,25);
   tempStr += str[j];
}
